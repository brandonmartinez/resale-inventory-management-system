{
    "githubIssues.issueBranchTitle": "${user}/issue/${issueNumber}-${sanitizedIssueTitle}",
    "githubIssues.queries": [
        {
            "label": "My Issues",
            "query": "default"
        },
        {
            "label": "Created Issues",
            "query": "author:${user} state:open repo:${owner}/${repository} sort:created-desc"
        },
        {
            "label": "Open Issues",
            "query": "state:open repo:${owner}/${repository} sort:created-desc"
        }
    ],
    "githubIssues.issueCompletions.enabled": false,
    "bashIde.explainshellEndpoint": "http://0.0.0.0:6000",
    "azureFunctions.deploySubpath": "src/rims-api",
    "azureFunctions.postDeployTask": "npm install (functions)",
    "azureFunctions.projectLanguage": "JavaScript",
    "azureFunctions.projectRuntime": "~3",
    "debug.internalConsoleOptions": "neverOpen",
    "azureFunctions.projectSubpath": "src/rims-api",
    "azureFunctions.preDeployTask": "npm prune (functions)"
}